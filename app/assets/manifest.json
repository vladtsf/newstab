{
  "manifest_version": 2,

  "name": "Newstab",
  "description": "This extension show the breaking news on newtab page.",
  "content_security_policy": "script-src 'self' chrome-extension-resource:; object-src 'self'",

  "version": "0.0.0",

  "permissions": [
    "http://localhost:3000/"
  ],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}